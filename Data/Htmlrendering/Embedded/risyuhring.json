{
  "img": {
    "@type": "codeCss",
    "lastReviewed": "2025-11-16",
    "supplementaryExplanation": "この要素は、置換要素です。",
    "code": [
      {"img:is([sizes='auto' i], [sizes^='auto,' i])": {
        "contain": "size !important",
        "contain-intrinsic-size": "300px 150px"
      }},
      {"img[align=left i]": {
        "margin-right": "3px",
        "float": "left"
      }},
      {"img[align=right i]": {
        "margin-left": "3px",
        "float": "right"
      }},
      {"img[align=top i]": {
        "vertical-align": "top"
      }},
      {"img[align=baseline i]": {
        "vertical-align": "baseline"
      }},
      {"img[align=texttop i]": {
        "vertical-align": "text-top"
      }},
      {"img[align=absmiddle i], img[align=abscenter i]": {
        "vertical-align": "middle"
      }},
      {"img[align=bottom i]": {
        "vertical-align": "bottom"
      }}
    ],
    "reference": [
      {
        "title":   "HTML Living Standard. 15 Rendering 15.4 Replaced elements. Last Updated 13 November 2025",
        "href":    "https://html.spec.whatwg.org/multipage/rendering.html#replaced-elements",
        "heading": "HTML Living Standard. 15 Rendering 15.4 Replaced elements",
        "sub":     "最終更新日 2025年11月13日"
      },
      {
        "title":   "HTML Living Standard. 15 Rendering 15.4.2 Images. Last Updated 13 November 2025",
        "href":    "https://html.spec.whatwg.org/multipage/rendering.html#images-3",
        "heading": "HTML Living Standard. 15 Rendering 15.4.2 Images",
        "sub":     "最終更新日 2025年11月13日"
      }
    ]
  }
}
