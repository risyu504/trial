{
  "summary": {
    "@type": "codeCss",
    "lastReviewed": "2025-11-16",
    "code": [
      {"summary": {
        "display": "block"
      }},
      {"details > summary:first-of-type": {
        "display": "list-item",
        "counter-increment": "list-item 0",
        "list-style": "disclosure-closed inside"
      }},
      {"details[open] > summary:first-of-type": {
        "list-style-type": "disclosure-open"
      }}
    ],
    "reference": [
      {
        "title":   "HTML Living Standard. 15 Rendering 15.5.5 The details and summary elements. Last Updated 13 November 2025",
        "href":    "https://html.spec.whatwg.org/multipage/rendering.html#the-details-and-summary-elements",
        "heading": "HTML Living Standard. 15 Rendering 15.5.5 The details and summary elements",
        "sub":     "最終更新日 2025年11月13日"
      }
    ]
  }
}
