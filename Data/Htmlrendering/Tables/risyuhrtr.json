{
  "tr": {
    "@type": "codeCss",
    "lastReviewed": "2025-11-16",
    "code": [
      {"tr": {
        "display": "table-row",
        "vertical-align": "inherit",
        "border-color": "inherit",
        "unicode-bidi": "isolate"
      }},
      {"tr[hidden]": {
        "display": "table-row",
        "visibility": "collapse"
      }},
      {"table > tr": {
        "vertical-align": "middle"
      }},
      {"tr[align=absmiddle i]": {
        "text-align": "center"
      }},
      {"tr[valign=top i]": {
        "vertical-align": "top"
      }},
      {"tr[valign=middle i]": {
        "vertical-align": "middle"
      }},
      {"tr[valign=bottom i]": {
        "vertical-align": "bottom"
      }},
      {"tr[valign=baseline i]": {
        "vertical-align": "baseline"
      }},
      {"table[rules=rows i] > tr, table[rules=rows i] > thead > tr, table[rules=rows i] > tbody > tr, table[rules=rows i] > tfoot > tr": {
        "border-block-width": "1px",
        "border-block-style": "solid"
      }}
    ],
    "reference": [
      {
        "title":   "HTML Living Standard. 15 Rendering 15.3.8 Tables. Last Updated 13 November 2025",
        "href":    "https://html.spec.whatwg.org/multipage/rendering.html#tables-2",
        "heading": "HTML Living Standard. 15 Rendering 15.3.8 Tables",
        "sub":     "最終更新日 2025年11月13日"
      }
    ]
  }
}
